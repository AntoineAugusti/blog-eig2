
#!/bin/bash

echo "Start deploy"
ssh -tq root@eig2.etalab.gouv.fr "bash -lc 'cd /var/www/blog-eig2 && git pull'"
ssh -tq root@eig2.etalab.gouv.fr "bash -lc 'cd /var/www/blog-eig2 && bundle exec jekyll doctor'"
ssh -tq root@eig2.etalab.gouv.fr "bash -lc 'cd /var/www/blog-eig2 && bundle exec jekyll build'"
echo "Deployed Successfully!"

exit 0
